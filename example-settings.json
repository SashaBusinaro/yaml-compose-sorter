{
  "yaml-compose-sorter.topLevelKeyOrder": [
    "version",
    "name",
    "services",
    "volumes",
    "networks",
    "custom-key",
    "configs",
    "secrets"
  ],
  "yaml-compose-sorter.serviceKeyOrder": [
    "container_name",
    "image",
    "build",
    "restart",
    "depends_on",
    "ports",
    "expose",
    "volumes",
    "environment",
    "env_file",
    "networks",
    "labels",
    "healthcheck"
  ],
  "yaml-compose-sorter.addDocumentSeparator": false,
  "yaml-compose-sorter.addBlankLinesBetweenTopLevelKeys": true,
  "yaml-compose-sorter.removeVersionKey": false,
  "yaml-compose-sorter.transformKeyValueLists": false,
  "yaml-compose-sorter.addBlankLinesBetweenServices": true,

  "// Configure Format On Save for Docker Compose": "To use this extension on save, add the following:",
  "[dockercompose]": {
    "editor.defaultFormatter": "SashaBusinaro.yaml-compose-sorter",
    "editor.formatOnSave": true
  }
}
